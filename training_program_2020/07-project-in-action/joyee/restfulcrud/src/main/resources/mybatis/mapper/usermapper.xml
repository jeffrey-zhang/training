<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.trendmicro.restfulcrud.dao.UserDao">
  <select id="getUser" resultType="com.trendmicro.restfulcrud.entity.User">
    select * from loginfo where username = #{username} and password = #{password} 
  </select>


  <!-- <insert id="insertUser" >
    insert into loginfo(username,password) values (#{username},#{password})
  </insert> -->

</mapper>








